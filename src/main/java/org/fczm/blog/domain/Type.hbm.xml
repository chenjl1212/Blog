<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.fczm.blog.domain">
	<class name="Type" table="type">
		<id name="tid" type="string">
			<generator class="uuid"/>
		</id>
		<property name="tname" type="string" not-null="true"/>
		<property name="date" type="java.util.Date" not-null="true"/>
		<property name="count" type="integer" not-null="true"/>
		<list name="blogs" cascade="delete" inverse="true">
			<key column="tid"  not-null="true"/>
			<index column="date" type="java.util.Date"/>
			<one-to-many class="Blog"></one-to-many>
		</list> 
	</class>
</hibernate-mapping>